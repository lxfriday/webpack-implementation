{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0
  }
}
